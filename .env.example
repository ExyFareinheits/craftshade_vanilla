# Discord OAuth
VITE_DISCORD_CLIENT_ID=your_discord_client_id
DISCORD_CLIENT_SECRET=your_discord_client_secret
VITE_DISCORD_GUILD_ID=your_discord_guild_id

# URLs (змінити на свій домен!)
VITE_APP_URL=http://localhost:5173
VITE_REDIRECT_URI=http://localhost:5173/auth/callback

# JWT Secret (згенерувати: node -e "console.log(require('crypto').randomBytes(64).toString('hex'))")
JWT_SECRET=your-super-secret-jwt-key

# Supabase (для клієнта - з VITE_ префіксом)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Supabase (для серверних API endpoints - без VITE_)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key
